{
    "layer": "top",
        "position": "top",
        "margin-bottom": 0,
        "modules-left": [ "wlr/workspaces", "wlr/taskbar"],
        "modules-center": [ ],
        "modules-right": [ "custom/media", "network", "memory", "cpu", "pulseaudio", "tray", "custom/powermenu", "clock" ],
        "tray": {
            "icon-size": 16,
            "spacing": 10
        },
        "wlr/taskbar": {
            "active-only": true
        },
        "wlr/taskbar": {
            "format": "{icon} {name}",
            //"max-length": 10,
            "icon-size": 16,
            "icon-theme": "Papirus-Dark",
            "tooltip-format": "{app_id}:{title:3}:{state}",
            "on-click": "minimize-raise",
            "on-click-middle": "close",
            "on-click-right": "maximize"
        },
        "network": {
            // "tooltip-format-ethernet": "{ifname} ÔÉÅ",
            "interface": "enp4s0",
            "tooltip-format": "ÔÉó {bandwidthDownBits} ÔÉò {bandwidthUpBits}",
            "interval": 2,
            "on-click": "exec kitty nmtui",
            "format": "{ifname} ÔÉÅ "
        },
        "temperature": {
            "format": "{icon} {temperatureC}¬∞C",
            "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
            "critical-threshold": 80,
            "format-icons": ["Ôãã", "Ôãâ", "Ôãá"]
        },
        "memory": {
            "interval": 2,
            "format": "Óâ¶ {:02}%",
            "tooltip": false
        },
        "cpu": {
            "format": "Ôãõ {usage:02}%",
            "tooltip": "false"
        },
        "custom/powermenu": {
            "format": "ÔÄë",
            "tooltip": false,
            "on-click": "exec wlogout -p layer-shell"
        },
        "custom/updates": {
            "format": "<span color=\"#61afef\">ÔÇ™</span> {}",
            "tooltip": false,
            "interval": 3600,
            "exec": "exec $HOME/.config/waybar/checkupdate.sh"
        },
        "pulseaudio": {
            "format": "{volume}% {icon}",
            "format-bluetooth": "{volume}% {icon}Ôäî",
            "format-muted": "ÔÄ¶",
            "format-icons": {
                "headphone": "ÔÄ•",
                "hands-free": "Ôñê",
                "headset": "Ôñê",
                "phone": "ÔÇï",
                "portable": "ÔÇï",
                "car": "ÔÜπ",
                "default": ["ÔÄß", "ÔÄ®"]
            },
            "scroll-step": 5,
            "on-click": "pavucontrol"
        },
        "clock": {
            "interval": 60,
            "format": "\uf017 {:%H:%M}",
            "format-alt": "\uf133 {:%Y-%m-%d %H:%M}",
            "tooltip": true,
            "tooltip-format": "<big>{:%B %Y}</big>\n<tt>{calendar}</tt>"
        },
    "custom/media": {
        "format": "{icon} {0}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "on-click": "playerctl play-pause",
        "smooth-scrolling-threshold": 10,
        "on-scroll-up": "playerctl position 10+",
        "on-scroll-down": "playerctl position 10-",
        "escape": true,
        "exec": "$HOME/.config/waybar/scripts/mediaplayer.py 2> /dev/null"
    }
}

